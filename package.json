{
	"name": "turbo-mempool",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"utxo": "bun src/lib/utils/script_loadUnspetUtxo.ts > src/lib/data/utxos.ts",
		"compile": "npm run compile:buy && npm run compile:sell && npm run compile:swap && npm run compile:deposit",
		"compile:buy": "bun src/lib/server/turbo-mempool/contracts/buy-token-for-erg.ts > src/lib/server/constants/buyOrder.ts",
		"compile:sell": "bun src/lib/server/turbo-mempool/contracts/sell-token-for-erg.ts > src/lib/server/constants/sellOrder.ts",
		"compile:swap": "bun src/lib/server/turbo-mempool/contracts/swap-tokens.ts > src/lib/server/constants/swapOrder.ts",
		"compile:deposit": "bun src/lib/server/turbo-mempool/contracts/deposit-address.ts > src/lib/server/constants/depositAddress.ts",
		"dev": "vite dev",
		"server": "pm2 stop crystal-pool && pm2 start ecosystem.config.cjs && pm2 logs",
		"server:logs": "pm2 logs",
		"server:stop": "pm2 stop crystal-pool",
		"db:clear": "rm chain.db*",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/express": "^4.17.21",
		"@types/json-bigint": "^1.0.4",
		"@types/node": "^20.12.8",
		"@types/socket.io": "^3.0.2",
		"autoprefixer": "^10.4.19",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.4.1",
		"tsx": "^4.11.0",
		"typescript": "^5.4.5",
		"vite": "^5.0.3",
		"vite-plugin-wasm": "^3.3.0",
		"vitest": "^1.2.0"
	},
	"type": "module",
	"dependencies": {
		"@ergo-graphql/types": "^0.5.1",
		"@fleet-sdk/common": "^0.3.4",
		"@fleet-sdk/compiler": "^0.3.4",
		"@fleet-sdk/core": "^0.3.4",
		"@fleet-sdk/serializer": "^0.3.4",
		"bignumber.js": "^9.1.2",
		"bip32": "^4.0.0",
		"bip39": "^3.1.0",
		"body-parser": "^1.20.2",
		"bs58": "^5.0.0",
		"bs58check": "^3.0.1",
		"buffer": "^6.0.3",
		"ergo-lib-wasm-browser": "^0.26.0",
		"ergo-lib-wasm-nodejs": "^0.26.0",
		"express": "^4.19.2",
		"json-bigint": "^1.0.0",
		"rollup-plugin-node-polyfills": "^0.2.1",
		"safe-buffer": "^5.2.1",
		"socket.io": "^4.7.5",
		"socket.io-client": "^4.7.5",
		"sqlite": "^5.1.1",
		"sqlite3": "^5.1.7",
		"tiny-secp256k1": "^2.2.3"
	}
}
