{
	"name": "turbo-mempool",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"utxo": "bun src/lib/utils/script_loadUnspetUtxo.ts > src/lib/data/utxos.ts",
		"compile": "bun src/lib/compiler/update-address-constants.ts",
		"dev": "vite dev",
		"restart": "npm run db:clear || npm run server:stop && npm run server",
